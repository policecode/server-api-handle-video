{
  "name": "fvn-automation-private",
  "version": "4.0.3",
  "description": "Automation tool for private client",
  "main": "app.js",
  "repository": "https://github.com/hixbotay",
  "license": "MIT",
  "keywords": [],
  "type": "commonjs",
  "scripts": {
    "version": "standard-version",
    "reset-hard": "git clean -dfx && git reset --hard && npm i",
    "run-one": "ts-node src/runOne.js",
    "build": "run-p build:*",
    "build:ui": "cd ./client/ui && npm run build",
    "package": "npm run build && electron-forge package",
    "make": "electron-forge make",
    "dev": "nodemon --max-old-space-size=8192 --inspect ./app.js",
    "start": "node --max-old-space-size=8192 --inspect ./app.js"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@bitauth/libauth": "^1.17.1",
    "@distube/ytdl-core": "^4.16.12",
    "await-to-js": "^3.0.0",
    "axios": "^1.5.1",
    "bluebird": "^3.7.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "fingerprint-generator": "^2.1.21",
    "fingerprint-injector": "^2.1.21",
    "fs-extra": "^10.1.0",
    "gaxios": "^5.0.2",
    "ghost-cursor": "^1.1.18",
    "mouse-helper": "^1.0.6",
    "os-tmpdir": "2.0.0",
    "proxy-agent-v2": "^1.0.3",
    "ps": "^1.0.0",
    "ps-list": "^8.1.1",
    "puppeteer": "^23.3.1",
    "puppeteer-core": "^19.7.5",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-anonymize-ua": "^2.4.4",
    "puppeteer-extra-plugin-fingerprinter": "^1.0.1",
    "puppeteer-extra-plugin-stealth": "^2.11.1",
    "puppeteer-page-proxy": "^1.3.0",
    "puppeteer-real-browser": "^1.4.2",
    "request": "^2.88.2",
    "sequelize": "^6.32.1",
    "shelljs": "^0.8.5",
    "socket.io": "^4.5.4",
    "sqlite3": "^5.1.6",
    "ts-retry": "^4.1.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "xlsx": "^0.18.5",
    "yaml": "^2.2.1"
  },
  "devDependencies": {
    "cspell": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.38.0",
    "gh-pages": "^3.1.0",
    "nodemon": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "or": "^0.2.0",
    "prettier": "^2.1.1",
    "standard-version": "^9.0.0"
  },
  "files": [
    "!**/*.spec.*",
    "!**/*.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": true
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  },
  "build": {
    "asar": true
  }
}

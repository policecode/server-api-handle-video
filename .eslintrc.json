{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": false,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "script"
  },
  "rules": {
    "no-unused-vars": 1,
    "no-undef": 1,
    "no-extra-semi": "warn",
    "no-empty": "warn",
    "no-async-promise-executor": "warn",
    "no-constant-condition": "warn",
    "no-case-declarations": "off"

  }
}
